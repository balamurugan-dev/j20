{
  "name": "j20",
  "displayName": "J2O - Json to Dart (freezed)",
  "description": "Json to Dart classes converter including freezed classes",
  "version": "0.0.3",
  "publisher": "BalaDhruv",
  "author": {
    "name": "Balamurugan",
    "email": "androbalamail@gmail.com",
    "url": "https://balamurugan.dev/"
  },
  "homepage": "https://github.com/balamurugan-dev/j20/tree/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/balamurugan-dev/j20/tree/main"
  },
  "bugs": {
    "url": "https://github.com/balamurugan-dev/j20/issues"
  },
  "icon": "media/logo/logo.png",
  "keywords": [
    "dartclass",
    "jsontodart",
    "Convertor",
    "json"
  ],
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-to-dart.convertJSON",
        "title": "J2O: JSON to DART"
      },
      {
        "command": "json-to-dart.addVariableInFreezed",
        "title": "J2O: Add Variable in Freezed Class"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "json-to-dart.addVariableInFreezed",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "json-to-dart.addVariableInFreezed",
          "group": "J2O@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
