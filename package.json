{
  "name": "j20",
  "displayName": "j20",
  "description": "convert json into dart",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:j20.convertFromClipboard",
		"onCommand:j20.convertFromClipboardToFile"
		
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == dart",
					"command": "j20.convertFromClipboardToFile",
					"group": "jsontodart@1"
				},
				{
					"when": "explorerResourceIsFolder",
					"command": "j20.convertFromClipboardToFolder",
					"group": "jsontodart@1"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == dart",
					"command": "j20.convertFromClipboardToFile",
					"group": "jsontodart@1"
				}
			]
		},
    "commands": [
      {
        "command": "j20.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "j20.convertFromClipboardToFolder",
        "title": "Convert From Folder"
      },
      {
        "command": "j20.convertFromClipboardToFile",
        "title": "Convert From File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "yaml": "^2.5.0"
  }
}
